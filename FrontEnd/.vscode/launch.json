{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Npm Webpack",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot} npm run serve",
            "sourceMaps": true
        },
        {
            "name": "Attach to Chrome, with sourcemaps",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        }
    ]
}